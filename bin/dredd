#!/usr/bin/env node

// When changing this file, please don't forget to make corresponding changes
// also in ./bin/dredd-coverage.

var DreddCommand = require('../lib/dredd-command');


var dreddCli = new DreddCommand({
  custom: {
    cwd: process.cwd(),
    argv: process.argv.slice(2)
  },
  exit: process.exit
});


dreddCli.run();
